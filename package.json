{
  "name": "destiny-key-app",
  "version": "1.0.0",
  "description": "命運之鑰 - 八星磁場數字能量分析平台",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "concurrently \"vite\" \"tsx watch server/_core/index.ts\"",
    "build": "vite build && esbuild server/_core/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "preview": "vite preview",
    "start": "node dist/index.js",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.62.11",
    "@trpc/client": "^11.0.0-rc.553",
    "@trpc/react-query": "^11.0.0-rc.553",
    "@trpc/server": "^11.0.0-rc.553",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "wouter": "^3.3.5",
    "express": "^4.21.2",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.36.4",
    "mysql2": "^3.11.5",
    "openai": "^4.77.3",
    "zod": "^3.24.1",
    "lucide-react": "^0.468.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/express": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-react": "^4.3.4",
    "vite": "^6.0.3",
    "typescript": "^5.7.2",
    "tsx": "^4.19.2",
    "esbuild": "^0.24.2",
    "drizzle-kit": "^0.30.1",
    "tailwindcss": "^3.4.17",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "concurrently": "^9.1.2"
  }
}
